<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane fx:id="pane" prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Profile">
   <children>
      <ImageView fitHeight="450.0" fitWidth="600.0" layoutX="-1.0" pickOnBounds="true">
         <image>
            <Image url="@../PNG/NEW/1300558.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="nickName" layoutX="12.0" layoutY="160.0" opacity="0.85" prefHeight="33.0" prefWidth="170.0" promptText="New Nick Name">
         <font>
            <Font name="Yu-Gi-Oh! ITC Stone Serif LT Italic" size="14.0" />
         </font>
      </TextField>
      <Button layoutX="34.0" layoutY="298.0" mnemonicParsing="false" onMouseClicked="#changeNickname" prefHeight="53.0" prefWidth="141.0" style="-fx-background-size: 140;" styleClass="button1" stylesheets="@../CSS/Buttons.css" text="Change Nick Name">
         <font>
            <Font name="Yu-Gi-Oh! Matrix Small Caps 1" size="18.0" />
         </font>
      </Button>
      <Button layoutX="229.0" layoutY="298.0" mnemonicParsing="false" onMouseClicked="#changePassword" prefHeight="53.0" prefWidth="141.0" style="-fx-background-size: 140;" styleClass="button1" stylesheets="@../CSS/Buttons.css" text="Change Password">
         <font>
            <Font name="Yu-Gi-Oh! Matrix Small Caps 1" size="18.0" />
         </font>
      </Button>
      <Button layoutX="455.0" layoutY="303.0" mnemonicParsing="false" onMouseClicked="#leftAvatar" prefHeight="44.0" prefWidth="44.0" style="-fx-background-size: 43.3;" styleClass="leftArrow" stylesheets="@../CSS/Buttons.css" />
      <Button layoutX="502.0" layoutY="303.0" mnemonicParsing="false" onMouseClicked="#rightAvatar" prefHeight="44.0" prefWidth="44.0" style="-fx-background-size: 43.2;" styleClass="rightArrow" stylesheets="@../CSS/Buttons.css" />
      <Button layoutX="12.0" layoutY="398.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="44.0" prefWidth="44.0" style="-fx-background-size: 44;" styleClass="buttons" stylesheets="@../CSS/Buttons.css" textFill="RED" />
      <ImageView fx:id="avatar" fitHeight="130.0" fitWidth="115.0" layoutX="445.0" layoutY="19.0" pickOnBounds="true" />
      <Label fx:id="text" alignment="TOP_LEFT" layoutX="212.0" layoutY="393.0" prefHeight="53.0" prefWidth="176.0" textAlignment="JUSTIFY" textFill="#590000" wrapText="true">
         <font>
            <Font name="Yu-Gi-Oh! ITC Stone Serif LT Italic" size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="oldPassword" layoutX="209.0" layoutY="160.0" opacity="0.85" prefHeight="33.0" prefWidth="176.0" promptText="Password">
         <font>
            <Font name="Yu-Gi-Oh! ITC Stone Serif LT Italic" size="14.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="newPassword" layoutX="209.0" layoutY="234.0" opacity="0.85" prefHeight="33.0" prefWidth="176.0" promptText="New Password">
         <font>
            <Font name="Yu-Gi-Oh! ITC Stone Serif LT Italic" size="14.0" />
         </font>
      </PasswordField>
      <Button fx:id="userName" cancelButton="true" disable="true" layoutX="220.0" layoutY="48.0" mnemonicParsing="false" opacity="0.92" prefHeight="72.0" prefWidth="160.0" textFill="#0e1466" textOverrun="CLIP" wrapText="true">
         <font>
            <Font name="Yu-Gi-Oh! ITC Stone Serif LT Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="userNickName" cancelButton="true" centerShape="false" disable="true" layoutX="17.0" layoutY="48.0" mnemonicParsing="false" opacity="0.92" prefHeight="72.0" prefWidth="160.0" textFill="#0e1466" wrapText="true">
         <font>
            <Font name="Yu-Gi-Oh! ITC Stone Serif LT Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="imageChooser" layoutX="429.0" layoutY="241.0" mnemonicParsing="false" onAction="#fileChooser" prefHeight="53.0" prefWidth="141.0" style="-fx-background-size: 140;" styleClass="button1" stylesheets="@../CSS/Buttons.css" text="Choose Image From Computer" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Yu-Gi-Oh! ITC Stone Serif LT Italic" size="12.0" />
         </font>
      </Button>
   </children>
</Pane>
